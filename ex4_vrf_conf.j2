{%- for vrf_name,rd_number in vrf_list.items() %}
------------------------------- 
vrf definition {{ vrf_name }}
 rd {{ rd_number }}
 {%- if ipv4_enabled == 1 %}
 !
 address-family ipv4
  route-target export {{ rd_number }}
  route-target import {{ rd_number }}
 exit-address-family
 {%- endif %}
 {%- if ipv6_enabled == 1 %}
 !
 address-family ipv6
  route-target export {{ rd_number }}
  route-target import {{ rd_number }}
 exit-address-family
 {%- endif %}
{%- endfor %} 
 
